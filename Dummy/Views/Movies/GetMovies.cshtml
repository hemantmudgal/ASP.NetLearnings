@model IEnumerable<Dummy.Models.Movies>
@{
	ViewData["Title"] = "View";
	Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table table-bordered table-hover">
	<thead>
		<tr>
			<th>Movies</th>
			<th>Genre</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var movie in Model)
		{
			<tr>
				<td>@Html.ActionLink(movie.Name, "Detail", "Movies", new { id = movie.Id }, null)</td>
				<td>@movie.Genre.Name</td>
			</tr>
		}
	</tbody>
</table>


